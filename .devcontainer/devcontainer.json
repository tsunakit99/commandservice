{
    "name": "SampleProgram CommandService" ,
    "dockerComposeFile":[
        "../docker-compose.yml"
    ],
    "service":"commandservice",
    "workspaceFolder": "/go/src/commandservice",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go"       
            ],
            "settings": {
                "go.gopath": "/go" ,
                "go.inferGopath": true ,
                "go.useLanguageServer": true
            }
        }
    },
    "mounts": [
        "source=${localEnv:SSH_AUTH_SOCK},target=/ssh-agent,type=bind"
    ],
    "remoteEnv": {
        "SSH_AUTH_SOCK": "/ssh-agent"
    }
}
